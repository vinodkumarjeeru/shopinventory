<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.shop.domain.Store" table="store">
        <id name="item_Id" column="item_Id" type="java.lang.Long">
            <generator class="native"/>
        </id>
        <property name="item_Name" column="item_Name" type="java.lang.String"></property>
        <property name="total_Items" column="total_Items" type="java.lang.Integer"></property>
        <property name="storage_Date" column="date" type="java.util.Date"></property>
        <property name="price" column="price" type="java.lang.Double"></property>
        <property name="soldItems" column="soldItems" type="java.lang.Integer"></property>
        <property name="remainingItems" column="remItems" type="java.lang.Integer"></property>
    </class>
</hibernate-mapping>
